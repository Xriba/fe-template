{
    "name": "fe-template",
    "private": true,
    "version": "0.1.0",
    "scripts": {
        "dev": "vite",
        "build:ci": "tsc && vite build",
        "serve": "vite preview",
        "prettier": "prettier --write .",
        "prettier-check": "prettier --check .",
        "stylelint": "stylelint **/*.css **/*.scss --cache",
        "stylelint-fix": "stylelint **/*.css **/*.scss --fix --cache",
        "stylelint-check": "stylelint-config-prettier-check",
        "lint": "eslint ./ --cache",
        "lint-fix": "eslint ./ --fix --cache",
        "prepare": "husky install",
        "test": "vitest run",
        "coverage": "vitest run --coverage",
        "e2e": "pnpm exec playwright test",
        "e2e:headed": "PWDEBUG=1 pnpm exec playwright test --headed",
        "e2e:ci": "pnpm exec playwright test --reporter=junit"
    },
    "dependencies": {
        "@bgotink/playwright-coverage": "^0.1.3",
        "@reduxjs/toolkit": "^1.8.1",
        "@xriba/ui": "^3.0.0-alpha.22",
        "date-fns": "^2.28.0",
        "dotenv": "^16.0.0",
        "i18next": "^21.6.16",
        "immer": "^9.0.12",
        "msw": "^0.39.2",
        "react": "^18.1.0",
        "react-dom": "^18.1.0",
        "react-i18next": "^11.16.7",
        "react-redux": "^8.0.1",
        "react-router-dom": "^6.3.0"
    },
    "devDependencies": {
        "@babel/core": "^7.17.9",
        "@playwright/test": "^1.21.1",
        "@popperjs/core": "^2.11.5",
        "@testing-library/jest-dom": "^5.16.4",
        "@testing-library/react": "^13.1.1",
        "@testing-library/react-hooks": "^7.0.2",
        "@types/lodash": "^4.14.182",
        "@types/node": "^17.0.29",
        "@types/react": "^18.0.8",
        "@types/react-dom": "^18.0.2",
        "@types/react-router-dom": "^5.3.3",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "@vitejs/plugin-react": "^1.3.1",
        "c8": "^7.11.2",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.5.0",
        "eslint-plugin-import": "^2.26.0",
        "eslint-plugin-node": "^11.1.0",
        "eslint-plugin-promise": "^6.0.0",
        "eslint-plugin-react": "^7.29.4",
        "eslint-plugin-react-hooks": "^4.5.0",
        "eslint-plugin-security": "^1.5.0",
        "happy-dom": "^3.1.0",
        "husky": "^7.0.4",
        "postcss": "^8.4.12",
        "prettier": "2.5.1",
        "prop-types": "^15.8.1",
        "sass": "^1.51.0",
        "stylelint": "^14.8.0",
        "stylelint-config-prettier": "^9.0.3",
        "stylelint-config-recommended-scss": "^6.0.0",
        "typescript": "^4.6.3",
        "vite": "^2.9.6",
        "vitest": "^0.9.4"
    }
}
