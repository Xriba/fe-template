    "eslint-plugin-react-hooks": "^4.4.0",
    "eslint-plugin-security": "^1.5.0",
    "husky": "^7.0.4",
    "postcss": "^8.4.12",
    "prettier": "2.5.1",
    "prop-types": "^15.8.1",
    "sass": "^1.50.1",
    "stylelint": "^14.7.1",
